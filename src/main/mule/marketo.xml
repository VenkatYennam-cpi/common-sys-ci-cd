	<sub-flow name="sf-post-marketo-lead-flow" doc:id="d7185c18-b6b1-46ea-9689-714a18509b6d">
		<logger level="INFO" doc:name="sf-post-marketo-lead-flow Begin" doc:id="ef31f13b-ef55-40e2-aa8c-dd0d907748ee" message="sf-post-marketo-lead-flow Begin" />
		<flow-ref doc:name="sf-get-marketo-oauth-access-token" doc:id="5f6dc816-97d4-47a4-9227-bd1888c54577" name="sf-get-marketo-oauth-access-token" target="marketoAccessToken" />
		<set-payload value="#[%dw 2.0 import * from dw::core::Binaries output application/java --- read((fromBase64(payload.content_data) as Binary),'application/octet-stream')]" doc:name="SP content data " />
		<compression:decompress  doc:name="Decompress the gzip data">
			<compression:decompressor>
				<compression:gzip-decompressor />
			</compression:decompressor>
		</compression:decompress>
		<logger level="DEBUG" category="DEBUG" doc:name="Decompress done successfully" doc:id="20202f82-87da-43b3-9708-c993e8f011af" message="File Decompressed" />
		<ee:transform doc:name="Marketo Create Job Response" doc:id="f4d64163-8c5d-4563-87f7-ced94fe0b56d">
			<ee:message>
				<ee:set-payload resource="dwScripts/marketo/p-marketo-request-payload.dwl" />
			</ee:message>
		</ee:transform>
		<until-successful maxRetries="${marketo.api.maxretries}" doc:name="Until Successful">
			<http:request method="POST" config-ref="HTTP_Request_Configuration_Marketo" path="bulk/v1/leads.json" doc:name="Marketo Create Lead" doc:id="89788abf-53b6-4bbc-9f7e-48c7f8821e71"
				outputMimeType="application/json">
				<reconnect frequency="${marketo.reconnect.frequency}" count="${marketo.reconnect.count}"/>
				<http:query-params><![CDATA[#[output application/java
					---
					{
						"access_token" : vars.marketoAccessToken.access_token,
						"format": vars.requestpayload.format,
						("listId": vars.configurationVar.list_id) if vars.configurationVar.list_id != null
					}]]]>
				</http:query-params>
			</http:request>
		</until-successful>
		<logger level="INFO" doc:name="sf-post-marketo-lead-flow End" doc:id="ef31f13b-ef55-40e2-aa8c-dd0d907748ee" message="sf-post-marketo-lead-flow End" />
