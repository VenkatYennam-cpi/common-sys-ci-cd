		<ee:transform doc:name="DW for storing request and configurations" doc:id="ee7b20e7-d232-46b8-9e60-246ab8e98c3c">
			<ee:variables>
				<ee:set-variable variableName="configurationVar" resource="dwScripts/marketo/v-configuration.dwl" />
				<ee:set-variable variableName="batchIdVar" resource="dwScripts/marketo/v-batchid.dwl" />
			</ee:variables>
		</ee:transform>
		<flow-ref doc:name="sf-get-marketo-oauth-access-token" doc:id="7686d510-bf94-484f-9555-bce04701ef9a" name="sf-get-marketo-oauth-access-token" target="marketoAccessToken" />
		<until-successful maxRetries="${marketo.api.maxretries}" doc:name="Until Successful">
			<http:request method="GET" config-ref="HTTP_Request_Configuration_Marketo" path="#['bulk/v1/leads/batch/' ++ vars.batchIdVar ++ '.json']" doc:name="Marketo Create Lead"
				doc:id="2b2d8f2b-c7d7-485d-91b9-3997fcb32750" outputMimeType="application/json">
				<http:query-params><![CDATA[#[output application/java
					---
					{
						"access_token" : vars.marketoAccessToken.access_token
					}]]]>
				</http:query-params>
			</http:request>
		</until-successful>
		<logger level="DEBUG" message="Get job status payload is ::::#[payload]" doc:name="Log Get job status payload" category="DEBUG" />

		<choice doc:name="CHECK IF Success" doc:id="da373f63-3694-4508-b1dc-28328e6fc898">
			<when expression="#[!payload.success]">
				<set-variable value='#[payload.errors.message[0]]' doc:name="Set Error Description" doc:id="acd73bd6-5acd-4638-b703-b31c429f0c49" variableName="errorDescription" />
				<ee:transform doc:id="069f6eb4-a5fa-4c14-b722-37940cc0c92f" doc:name="DW ErrorResponse request">
					<ee:message>
						<ee:set-payload resource="dwScripts/errorResponse400.dwl" />
					</ee:message>
					<ee:variables>
						<ee:set-variable variableName="httpStatus"><![CDATA[400]]></ee:set-variable>
					</ee:variables>
				</ee:transform>
			</when>
			<otherwise>
				<ee:transform doc:name="Set lead status response message" doc:id="aa781059-ff57-4294-930e-211f04f40048">
					<ee:message>
						<ee:set-payload resource="dwScripts/marketo/p-lead-status-response.dwl" />
					</ee:message>
				</ee:transform>
			</otherwise>
		</choice>
		<logger level="INFO" doc:name="mf-get-marketo-job-status-flow End" doc:id="3c963b60-4d36-47f3-b984-c617ffd05104" message="mf-get-marketo-job-status-flow End" />
	</sub-flow>

	<sub-flow name="sf-get-marketo-oauth-access-token" doc:id="c1c885e5-caea-4af7-b5c5-13210b44ee51">
		<logger level="INFO" doc:name="GET Marketo Oauth Access Token" doc:id="419909f5-7a14-49eb-94c5-550688b7afb1" message="Inside get-marketo-oauth-access-token flow Begin" />
